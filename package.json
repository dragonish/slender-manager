{
  "name": "slender-manager",
  "private": true,
  "version": "1.0.4",
  "type": "module",
  "scripts": {
    "dev": "cross-env GO_MODE=development vite",
    "pro": "cross-env GO_MODE=production vite",
    "check": "vue-tsc",
    "build": "vue-tsc && vite build",
    "build:zip": "vue-tsc && cross-env ZIP_MODE=1 vite build",
    "preview": "vite preview",
    "release": "standard-version",
    "alpha": "pnpm run release --prerelease alpha",
    "beta": "pnpm run release --prerelease beta",
    "test": "vue-tsc && cross-env NODE_NO_WARNINGS=1 mocha",
    "test:unit": "cross-env NODE_NO_WARNINGS=1 mocha",
    "i18n": "tsx ./tools/i18n.ts"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.2.6",
    "await-to-js": "^3.0.0",
    "axios": "^1.8.4",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.2",
    "vue-request": "^2.0.4",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.1",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "chai": "^5.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "less": "^4.2.2",
    "less-loader": "^12.2.0",
    "lodash-es": "^4.17.21",
    "mocha": "^11.1.0",
    "standard-version": "^9.5.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.3",
    "vite-plugin-zip-pack": "^1.2.4",
    "vue-tsc": "^2.2.8"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  }
}

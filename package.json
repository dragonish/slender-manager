{
  "name": "slender-manager",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env GO_MODE=development vite",
    "pro": "cross-env GO_MODE=production vite",
    "check": "vue-tsc",
    "build": "vue-tsc && vite build",
    "build:zip": "vue-tsc && cross-env ZIP_MODE=1 vite build",
    "preview": "vite preview",
    "release": "standard-version",
    "alpha": "pnpm run release --prerelease alpha",
    "beta": "pnpm run release --prerelease beta",
    "test": "vue-tsc && cross-env NODE_NO_WARNINGS=1 mocha"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.6.8",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.10",
    "file-saver": "^2.0.5",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/file-saver": "^2.0.7",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "chai": "^5.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "mocha": "^10.4.0",
    "standard-version": "^9.5.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.7",
    "vite-plugin-zip-pack": "^1.2.2",
    "vue-tsc": "^2.0.7"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    },
    "scripts": {
      "posttag": "pnpm run build:zip"
    }
  }
}

{
  "name": "slender-manager",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env GO_MODE=development vite",
    "pro": "cross-env GO_MODE=production vite",
    "check": "vue-tsc",
    "build": "vue-tsc && vite build",
    "build:zip": "vue-tsc && cross-env ZIP_MODE=1 vite build",
    "preview": "vite preview",
    "release": "commit-and-tag-version",
    "alpha": "pnpm run release --prerelease alpha",
    "beta": "pnpm run release --prerelease beta",
    "test": "vue-tsc && cross-env NODE_NO_WARNINGS=1 mocha",
    "test:unit": "cross-env NODE_NO_WARNINGS=1 mocha",
    "i18n": "tsx ./tools/i18n.ts"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.2.6",
    "await-to-js": "^3.0.0",
    "axios": "^1.9.0",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "pinia": "^3.0.2",
    "vue": "^3.5.16",
    "vue-i18n": "^11.1.10",
    "vue-request": "^2.0.4",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/chai": "^5.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "chai": "^5.2.0",
    "commit-and-tag-version": "^12.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "less": "^4.3.0",
    "less-loader": "^12.3.0",
    "lodash-es": "^4.17.21",
    "mocha": "^11.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.6",
    "vite-plugin-zip-pack": "^1.2.4",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.10"
  },
  "commit-and-tag-version": {
    "skip": {
      "changelog": true
    }
  }
}

{
  "name": "slender-manager",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env GO_MODE=development vite",
    "pro": "cross-env GO_MODE=production vite",
    "check": "vue-tsc",
    "build": "vue-tsc && vite build",
    "build:zip": "vue-tsc && cross-env ZIP_MODE=1 vite build",
    "preview": "vite preview",
    "release": "standard-version",
    "alpha": "pnpm run release --prerelease alpha",
    "beta": "pnpm run release --prerelease beta",
    "test": "vue-tsc && cross-env NODE_NO_WARNINGS=1 mocha",
    "i18n": "tsx ./tools/i18n.ts"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.2.3",
    "await-to-js": "^3.0.0",
    "axios": "^1.7.2",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.11",
    "file-saver": "^2.0.5",
    "pinia": "^2.1.7",
    "vue": "^3.4.29",
    "vue-i18n": "^9.13.1",
    "vue-request": "^2.0.4",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "chai": "^5.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lodash-es": "^4.17.21",
    "mocha": "^10.4.0",
    "standard-version": "^9.5.0",
    "tsx": "^4.15.5",
    "typescript": "^5.4.5",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.3.1",
    "vite-plugin-zip-pack": "^1.2.3",
    "vue-tsc": "^2.0.21"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    },
    "scripts": {
      "posttag": "pnpm run build:zip"
    }
  }
}
